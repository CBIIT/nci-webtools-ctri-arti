    "express": "^5.1.0",
    "express-session": "^1.18.1",
    "lodash": "^4.17.21",
    "mammoth": "^1.9.1",
    "multer": "^2.0.1",
    "node-cron": "^4.2.0",
    "node-forge": "^1.3.1",
